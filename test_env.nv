// Test 3: Check environment and basic process info
use std.env;
use std.process;

fn main() throws {
    // Environment variables
    let home = env.get("HOME");
    println(`HOME: ${home || "not set"}`);
    
    // Args (use std.process.args, not env.args)
    let args = process.args();
    println(`Args count: ${args.len()}`);
}
